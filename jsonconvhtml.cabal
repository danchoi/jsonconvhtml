  build-depends:       base >=4.6 && <4.8
                     , aeson >= 0.8.0.0
                     , bytestring
                     , attoparsec >= 0.12.0.0
                     , containers
                     , process
                     , unordered-containers
                     , text >= 1.0.0.0
                     , vector
